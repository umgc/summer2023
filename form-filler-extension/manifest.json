{
    "name": "ConvoBuddy Form Fill-In",
    "version": "0.0.1",
    "description": "",
    "manifest_version": 3,
    "author": "Charlie McAllister - Team A",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs"
    ],

    "action": {
        "default_popup": "popup.html",
        "default-title": "Form Filler"
    },

    "host_permissions": ["<all_urls>"],

    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
}
